<table class="border table table-striped-columns">
  <thead>
    <th scope="row">ジャンル検索</th>
  </thead>
  <tbody>
    <% Genre.all.each do |genre| %>
      <tr>
        <td>
          <%= link_to genre.name, events_path(genre_name: genre.name), class:"text-dark" %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>